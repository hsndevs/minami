	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--dark)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--manrope)",
					"fontSize": "20px",
					"fontWeight": "500",
					"lineHeight": "28px"
				}
			},
			"core/group": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			}
		},
		"color": {
			"background": "#f7f7f7",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--orange)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "18px",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "36px"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "32px"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "24px"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "20px"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "18px"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			}
		},
		"spacing": {
			"padding": {
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "16px",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.5",
			"textDecoration": "none",
			"textTransform": "none"
		}
	},
